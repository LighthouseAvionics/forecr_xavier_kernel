# For DSBOARD_ORNX_LAN with Orin NX SoM, type these commands below:
	mkdir $HOME/kernel_out_dsboard_ornx_lan_orin_nx
	cp $KERNEL_OUTPUT/arch/arm64/boot/Image $HOME/kernel_out_dsboard_ornx_lan_orin_nx/Image
	cp nvidia-oot/device-tree/platform/generic-dts/dtbs/tegra234-p3768-0000+p3767-0000-nv-dsboard-ornx-lan-base.dtb $HOME/kernel_out_dsboard_ornx_lan_orin_nx/tegra234-p3768-0000+p3767-0000-nv.dtb
	cp nvidia-oot/device-tree/platform/generic-dts/dtbs/tegra234-p3768-0000+p3767-0001-nv-dsboard-ornx-lan-base.dtb $HOME/kernel_out_dsboard_ornx_lan_orin_nx/tegra234-p3768-0000+p3767-0001-nv.dtb
	cp nvidia-oot/device-tree/platform/generic-dts/dtbs/tegra234-p3768-0000+p3767-0000-nv-dsboard-ornx-lan.dtb $HOME/kernel_out_dsboard_ornx_lan_orin_nx/0_tegra234-p3768-0000+p3767-0000-nv.dtb
	cp nvidia-oot/device-tree/platform/generic-dts/dtbs/tegra234-p3768-0000+p3767-0001-nv-dsboard-ornx-lan.dtb $HOME/kernel_out_dsboard_ornx_lan_orin_nx/0_tegra234-p3768-0000+p3767-0001-nv.dtb
	cp nvidia-oot/device-tree/platform/generic-dts/dtbs/tegra234-p3767-camera-dsboard-ornx-imx219.dtbo $HOME/kernel_out_dsboard_ornx_lan_orin_nx/
	cp nvidia-oot/device-tree/platform/generic-dts/dtbs/tegra234-p3767-camera-dsboard-ornx-imx477.dtbo $HOME/kernel_out_dsboard_ornx_lan_orin_nx/
	export INSTALL_MOD_PATH=$HOME/nvidia/nvidia_sdk/JetPack_6.0_Linux_JETSON_ORIN_NX_TARGETS/Linux_for_Tegra/rootfs/
	cd $INSTALL_MOD_PATH/..
	sudo ./apply_binaries.sh
	cd $OLDPWD
	sudo -E make install -C kernel
	cd $INSTALL_MOD_PATH
	sudo rm -rf ./lib/modules/5.15.136-tegra/build
	sudo rm -rf ./lib/modules/5.15.136-tegra/source
	sudo tar --owner root --group root -cjf kernel_supplements.tbz2 lib/modules
	sync
	cd $OLDPWD
	cp $INSTALL_MOD_PATH/kernel_supplements.tbz2 $HOME/kernel_out_dsboard_ornx_lan_orin_nx/


